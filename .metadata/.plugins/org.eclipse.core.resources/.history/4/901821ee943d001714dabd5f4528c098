
public class MaxPositive {

	public static void main(String[] args) {
		int n = 1000;
		boolean found = false;
		int maxPositive = 0;
		for (int i = n; i >= 0; i--) {
			if (i / 3 == 2 && (n-i) / 5 == 3 && i / 11 == 9 && !found) {
				maxPositive = i;
				found = true;
			}
		}
		
		System.out.println("Max positive:" + maxPositive);
	}

}
