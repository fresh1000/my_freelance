package source;

public class City extends AnyData {
	String cityName;

	public City(String name, String cityName) {
		this.name = name;
		this.cityName = cityName;
	}

	public Dlg showDialog(boolean b) {
		DlgCity d = new DlgCity();
		d.setEditable(b);
		d.setVisible(true);
		return d;
	}

	public Dlg showSonDialog() {
		return null;
	}
	
}
