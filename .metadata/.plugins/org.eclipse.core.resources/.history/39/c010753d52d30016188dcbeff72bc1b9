package source;
import javax.swing.JLabel;
import javax.swing.JTextField;

public class DlgMinistry extends Dlg{
	private JTextField textFieldNameMinistry;

	public DlgMinistry() {
		super();
		setBounds (100, 100, 331, 119);
		
		JLabel lblNewLabel = new JLabel("Name minister");
		contentPanel.add(lblNewLabel);
		
		textFieldNameMinistry = new JTextField();
		contentPanel.add(textFieldNameMinistry);
		textFieldNameMinistry.setColumns(10);
	}

	public Object createObject() throws Exception {
		if(!ok) return null;
		String fioMinister = textFieldNameMinistry.getText();

	}

}
