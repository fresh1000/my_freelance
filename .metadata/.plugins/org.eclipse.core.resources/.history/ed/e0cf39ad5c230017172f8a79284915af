
public class Subject {
	private String sCode;
	private String session;
	private int year;
	
	public Subject() {
		this.sCode = null;
		this.session = null;
		this.year = 0;
	}
	
	public Subject(Subject other) {
		
	}
	
	public Subject(String sCode, String session, int year) {
		this.sCode = sCode;
		this.session = session;
		this.year = year;
	}
	
}
